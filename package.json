{
  "name": "b2note-server",
  "version": "3.0.0",
  "description": "B2NOTE server",
  "private": "true",
  "main": "app/server.js",
  "scripts": {
    "generate": "scripts/generateSchemas.sh",
    "watch": "nodemon --watch app --ext ts,js,yaml --exec npm run build\\;npm run copy\\;npm run run",
    "build": "rm -rf dist/*; tsc; npm run copy",
    "copy": "cp .env app/core/api.yaml dist/",
    "run": "cd dist; node -r dotenv/config server",
    "clean": "npm cache clean --force"
  },
  "devDependencies": {
    "@babakness/exhaustive-type-checking": "^0.1.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/lodash": "^4.14.149",
    "@types/mongodb": "^3.3.14",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.5.0",
    "@types/passport": "^1.0.2",
    "@types/passport-http-bearer": "^1.0.34",
    "@types/pegjs": "^0.10.1",
    "@types/source-map-support": "^0.5.1",
    "@types/uuid": "^3.4.6",
    "source-map-support": "^0.5.16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roper79/b2note-nodejs"
  },
  "author": "Robert Pergl",
  "license": "Apache 2",
  "bugs": {
    "url": "https://github.com/roper79/b2note-nodejs/issues"
  },
  "homepage": "https://github.com/roper79/b2note-nodejs#readme",
  "dependencies": {
    "ajv": "^6.11.0",
    "axios": "^0.19.2",
    "chalk": "^3.0.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.6.6",
    "express-session": "^1.17.0",
    "lodash": "^4.17.15",
    "mongodb": "^3.5.2",
    "morgan": "^1.9.1",
    "openid-client": "^3.12.1",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-oauth": "^1.0.0",
    "pegjs": "^0.10.0",
    "uuid": "^3.4.0",
    "xmlbuilder": "^13.0.2"
  }
}
