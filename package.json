{
  "name": "b2note-server",
  "version": "3.0.0",
  "description": "B2NOTE server",
  "private": true,
  "main": "app/server.js",
  "scripts": {
    "generate": "scripts/generateSchemas.sh",
    "watch": "nodemon --watch app --ext ts,js,ejs,yaml,html --exec npm run build\\; npm run run",
    "build": "rm -rf dist/*; tsc && npm run copy",
    "copy": "cp -rv .env public app/views app/core/api.yaml dist/; cp -rv ../b2note-reactjs/dist dist/public/widget",
    "run": "cd dist; node -r dotenv/config server",
    "docker-build": "docker-compose build",
    "docker-tag": "docker tag b2note/b2note-nodejs b2note/b2note-nodejs:3.0-alpha4",
    "docker-push": "docker push b2note/b2note-nodejs",
    "docker-run": "docker-compose up --build",
    "docker-watch": "nodemon --watch app --ext ts,js,ejs,yaml,html --exec npm run build\\;npm run copy\\;npm run docker-run",
    "clean": "npm cache clean --force"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/express-session": "^1.17.0",
    "@types/lodash": "^4.14.149",
    "@types/mongodb": "^3.5.2",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.9.1",
    "@types/passport": "^1.0.3",
    "@types/passport-http-bearer": "^1.0.34",
    "@types/pegjs": "^0.10.1",
    "@types/source-map-support": "^0.5.1",
    "@types/uuid": "^7.0.0",
    "api-spec-converter": "^2.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roper79/b2note-nodejs"
  },
  "author": "Robert Pergl",
  "license": "Apache 2",
  "bugs": {
    "url": "https://github.com/roper79/b2note-nodejs/issues"
  },
  "homepage": "https://github.com/roper79/b2note-nodejs#readme",
  "dependencies": {
    "@babakness/exhaustive-type-checking": "^0.1.3",
    "ajv": "^6.12.0",
    "axios": "^0.19.2",
    "chalk": "^3.0.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.9.5",
    "express-session": "^1.17.0",
    "lodash": "^4.17.15",
    "mongodb": "^3.5.5",
    "morgan": "^1.9.1",
    "openid-client": "^3.14.0",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-oauth": "^1.0.0",
    "pegjs": "^0.10.0",
    "source-map-support": "^0.5.16",
    "uuid": "^7.0.2",
    "xmlbuilder": "^15.0.0"
  }
}
