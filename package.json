{
  "name": "b2note-api",
  "version": "3.7.0",
  "build": "1",
  "description": "B2NOTE server",
  "private": true,
  "scripts": {
    "generate": "scripts/generateSchemas.sh",
    "watch": "nodemon --watch app --ext ts,js --exec npm run build \\&\\& npm run start",
    "build": "rm -rf dist/*; tsc && npm run copy",
    "copy": "cp -rv .env public dist/",
    "start": "cd dist; node -r dotenv/config server",
    "docker-deploy": "scripts/git-tag.sh; scripts/docker-build.sh; scripts/docker-tag.sh; scripts/docker-push.sh",
    "docker-run": "docker-compose up --build",
    "docker-watch": "nodemon --watch app --ext ts,js,ejs,yaml,html --exec npm run build\\;npm run copy\\;npm run docker-run",
    "clean": "npm cache clean --force"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.155",
    "@types/mongodb": "^3.5.20",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.0.10",
    "@types/passport": "^1.0.3",
    "@types/passport-http-bearer": "^1.0.35",
    "@types/pegjs": "^0.10.2",
    "@types/source-map-support": "^0.5.1",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "api-spec-converter": "^2.11.0",
    "eslint": "^7.1.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@babakness/exhaustive-type-checking": "^0.1.3",
    "@types/jsonwebtoken": "^8.5.0",
    "ajv": "^6.12.2",
    "axios": "^0.19.2",
    "chalk": "^4.0.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongodb": "^3.5.8",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "pegjs": "^0.10.0",
    "qs": "^6.9.4",
    "source-map-support": "^0.5.19",
    "ts-enum-util": "^4.0.1",
    "uuid": "^8.1.0",
    "xmlbuilder": "^15.1.1"
  }
}
