{
  "name": "b2note-server",
  "version": "2.0.0",
  "description": "B2NOTE server",
  "private": "true",
  "main": "app/server.js",
  "scripts": {
    "watch": "nodemon --watch app --ext ts,js,yaml --exec npm run build\\;npm run copy\\;npm run run",
    "build": "rm -rf dist/*; tsc; npm run copy",
    "copy": "cp app/shared/api.yaml dist/",
    "run": "cd dist; node server",
    "clean": "npm cache clean --force"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/lodash": "^4.14.144",
    "@types/mongodb": "^3.3.8",
    "@types/morgan": "^1.7.37",
    "@types/source-map-support": "^0.5.0",
    "source-map-support": "^0.5.16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roper79/b2note-nodejs"
  },
  "author": "Robert Pergl",
  "license": "Apache 2",
  "bugs": {
    "url": "https://github.com/roper79/b2note-nodejs/issues"
  },
  "homepage": "https://github.com/roper79/b2note-nodejs#readme",
  "dependencies": {
    "@types/node": "^12.12.5",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-openapi-validator": "^2.14.4",
    "lodash": "^4.17.15",
    "mongodb": "^3.3.3",
    "morgan": "^1.9.1"
  }
}
